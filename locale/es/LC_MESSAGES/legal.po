#: ../../legal/terms/i18n/enterprise_fr.rst:171
msgid "4.3 Service de migration"
msgstr ""

#: ../../legal/terms/i18n/enterprise_fr.rst:176
msgid "Service de migration du Logiciel"
msgstr ""

#: ../../legal/terms/i18n/enterprise_fr.rst:178
msgid ""
